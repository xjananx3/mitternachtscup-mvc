@model IEnumerable<Team>
@{
    
}

<h1>Angemeldete Teams</h1>

<div class="album py-5 bg-light">
    <div class="container">
        <div class="row">
            @foreach(var item in Model)
            {
                <div class="col-md-4">
                    <div class="card mb-4 box-shadow">
                        <div class="card-body">
                            <p class="card-text">@item.Id</p>
                            <p class="card-text">@item.Name</p>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</div>